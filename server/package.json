    "url": "https://github.com/jumboindex/E-commerce-app/issues"
  },
  "homepage": "https://github.com/jumboindex/E-commerce-app#readme",
  "devDependencies": {
    "faker": "^5.5.3",
    "nodemon": "^2.0.15"
  },
  "dependencies": {
    "dotenv": "^14.1.0",
    "knex": "^1.0.1",
    "objection": "^3.0.1",
    "pg": "^8.7.1"
  }
}
